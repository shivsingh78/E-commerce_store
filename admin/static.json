{
  "routes": [
    { "src": "/.*", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "**/*.js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    },
    {
      "source": "**/*.css",
      "headers": [
        { "key": "Content-Type", "value": "text/css" }
      ]
    },
    {
      "source": "**/*.svg",
      "headers": [
        { "key": "Content-Type", "value": "image/svg+xml" }
      ]
    },
    {
      "source": "**/*.png",
      "headers": [
        { "key": "Content-Type", "value": "image/png" }
      ]
    }
  ]
}
